{
  "extends": [
    "stylelint-config-wordpress",
    "stylelint-config-recommended",
    "stylelint-a11y/recommended",
    "stylelint-config-rational-order"
  ],
  "plugins": [
    "stylelint-scss",
    "stylelint-a11y",
    "stylelint-order"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true
  }
}

